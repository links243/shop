image {
  display: block;
  width: 100%;
  height: 100%;
}
.category {
  height: 194rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.categoryItem {
  width: 120rpx;
  height: 130rpx;
}

.floor_title {
  height: 90rpx;
  width: 750rpx;
}

.floor_content {
  padding: 16rpx;
}

.floor_content_img {
  float: left;
  width: 232rpx;
  height: 190rpx;
  margin-right: 10rpx;
}

.floor_content_img:nth-child(1) {
  height: 386rpx;
  width: 232rpx;
}
.floor_content_img:nth-child(3) {
  margin-right: 0;
  margin-bottom: 6rpx;
}
.floor_content_img:nth-child(5) {
  margin-right: 0;
}
.toTop{
  position: fixed;
  right:20rpx;
  bottom: 20rpx;
  font-size: 40rpx;
  color: #fff;
  background-color: #000;
  opacity: .3;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 60rpx;
  height: 60rpx;
}
/* 清除浮动 */
.clearfix:after {   
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
